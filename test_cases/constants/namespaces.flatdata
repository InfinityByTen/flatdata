/* This test tests that symbols are properly separated into namespaces
 */
namespace n{
const i8 FOO = 0;
const i8 FOO2 = 10;
}

namespace m{
const i8 FOO = 1;

struct Bar {
	@const( FOO )
	foo1 : i8;
	@const( .n.FOO )
	foo2 : i8;
	@const( .m.FOO )
	foo3 : i8;
	@invalid_value( FOO )
	bar1 : i8;
	@invalid_value( .n.FOO )
	bar2 : i8;
	@invalid_value( .m.FOO )
	bar3 : i8;
}
}